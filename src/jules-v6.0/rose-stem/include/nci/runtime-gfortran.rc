    [[NCI_GFORTRAN_BUILD]]
        inherit = BUILD, NCI
        pre-script = """
           module use ~access/modules
           module load openmpi/4.0.2
           module unload netcdf
           module load netcdf/4.7.4p
           module load fcm
           """
        [[[environment]]]
            ROSE_TASK_N_JOBS = 4
        [[[directives]]]
	    -l ncpus = 4
	    -l mem = 8GB
	    -l jobfs = 2GB
      -q = normal

